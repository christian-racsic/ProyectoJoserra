import org.springframework.stereotype.Controller;

@Controller
public class ClienteService {
	private static ArrayList<Cliente> clientes = new ArrayList<>();
	
	
	static {
		clientes.add(new Cliente("usuario1", "password1", "usuario1@example.com"));
		clientes.add(new Cliente("usuario2", "password2", "usuario2@example.com"));
		clientes.add(new Cliente("usuario3", "password3", "usuario3@example.com"));
		clientes.add(new Cliente("usuario4", "password4", "usuario4@example.com"));
	}
	
}
